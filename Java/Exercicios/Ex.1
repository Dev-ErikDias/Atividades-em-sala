package Ola;

public class Ex1 {

	public static void main(String[] args) {
		int n = 1;
		System.out.println("While");
		while(n <= 10) {
			System.out.println(n);
			n++;
		}
		n = 1;
		System.out.println("Do-While");
		do {
			System.out.println(n);
			n++;
		}while(n <= 10);
		
		
		System.out.println("For");
		for(int n1 = 1; n1 <= 10; n1++) {
			System.out.println(n1);
		}
	}

}
