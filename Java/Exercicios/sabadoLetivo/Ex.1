package sabadoLetivo1;

public class Ex1 {

	public static void main(String[] args) {	
		
		System.out.println("FOR:");
		for(int n = 1; n <= 19;n++) {
			System.out.println(n);
			n++;
		}
		
		int n = 1;
		System.out.println("WHILE:");
		while(n <= 19){
			System.out.println(n);
			n = n+2;
		}
		n = 1;
		System.out.println("DO-WHILE:");
		do{
			System.out.println(n);
			n = n+2;
		}while(n <= 19);
	}

}
